{
    "name": "laravel-routes/routes",
    "description": "This Laravel package creates Routes and generating a route classes based and make your routes clean and easy to navigate to all controller class.",
    "keywords": ["artisan", "laravel", "routes", "command", "routes-generator", "generator", "routes"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin lee",
            "email": "ph.hitachi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.34",
        "laravel/framework": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "symfony/finder": "~2.8|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
          "PhHitachi\\Routes\\": "src/"
        },
        "files": [
            "src/Helper/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhHitachi\\Routes\\ArtisanRoutesServiceProvider"
            ],
            "aliases": {
                "Routes": "PhHitachi\\Routes\\Facades\\Router"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
